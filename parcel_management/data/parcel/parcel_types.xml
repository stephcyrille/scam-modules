<odoo>
    <data noupdate="1">
        <record id="parcel_type_fresh_food" model="parcel.type">
            <field name="code">FFDS</field>
            <field name="name">Fresh Food</field>
            <field name="description">Fresh alimentation items that could be comestible.</field>
            <field name="active">True</field>
        </record>
        <record id="parcel_type_dry_food" model="parcel.type">
            <field name="code">DFDS</field>
            <field name="name">Dry Food</field>
            <field name="description">Dry food that could put in a storage for a long time.</field>
            <field name="active">True</field>
        </record>
        <record id="parcel_type_box" model="parcel.type">
            <field name="code">BOX</field>
            <field name="name">Box</field>
            <field name="description">Standard cardboard box for general parcels.</field>
            <field name="active">True</field>
        </record>
        <record id="parcel_type_envelope" model="parcel.type">
            <field name="code">ENV</field>
            <field name="name">Envelope</field>
            <field name="description">Flat envelope for documents and small items.</field>
            <field name="active">True</field>
        </record>
        <record id="parcel_type_bag" model="parcel.type">
            <field name="code">BAG</field>
            <field name="name">Bag</field>
            <field name="description">Flexible bag for soft or irregularly shaped items.</field>
            <field name="active">True</field>
        </record>
        <record id="parcel_type_pc_tab_etc" model="parcel.type">
            <field name="code">PC&amp;DIV</field>
            <field name="name">PC, Smartphones and Tablettes</field>
            <field name="description">PC, Desktops, Smartphones, tablettes, small devices and others.</field>
            <field name="active">True</field>
        </record>
        <record id="parcel_type_electronic" model="parcel.type">
            <field name="code">ELECTRO</field>
            <field name="name">Electronic devices</field>
            <field name="description">Electronics devices like printers, scanners, and so on.</field>
            <field name="active">True</field>
        </record>
        <record id="parcel_type_house_elec" model="parcel.type">
            <field name="code">HELEC</field>
            <field name="name">House Electronic</field>
            <field name="description">House electronics equipements (wash machine, dishes washer, Dry machine).</field>
            <field name="active">True</field>
        </record>
        <record id="parcel_type_crate" model="parcel.type">
            <field name="code">CRATE (Fournitures)</field>
            <field name="name">Crate</field>
            <field name="description">Wooden crate for fragile or valuable items.</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>