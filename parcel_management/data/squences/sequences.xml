<odoo>
    <data noupdate="1">
      <record id="parcel_expedition_sequence" model="ir.sequence">
        <field name="name">Parcel Reference</field>
        <field name="code">parcel.expedition</field>
        <field name="active">TRUE</field>
        <field name="prefix">PEXP/%(y)s/%(month)s/%(day)s/</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
        <field name="padding">3</field>
        <field name="implementation">standard</field>
      </record>

      <record id="parcel_reception_sequence" model="ir.sequence">
        <field name="name">Parcel Reception Reference</field>
        <field name="code">parcel.reception</field>
        <field name="active">TRUE</field>
        <field name="prefix">PRCP/%(y)s/%(month)s/%(day)s/</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
        <field name="padding">3</field>
        <field name="implementation">standard</field>
      </record>
    </data>
</odoo>
