<odoo>
  <data>
    <record id="list_view_parcel_type" model="ir.ui.view">
        <field name="name">parcel.type.list</field>
        <field name="model">parcel.type</field>
        <field name="arch" type="xml">
            <list string="Parcel type list" editable="top">
              <field name="code" />
              <field name="name" />
              <field name="description" />
              <field name="active" />
            </list>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_parcel_type">
      <field name="name">Parcel Types</field>
      <field name="res_model">parcel.type</field>
      <field name="view_mode">list</field>
    </record>

  </data>
</odoo>